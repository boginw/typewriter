{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name": "TypeWriter",
	"description": "Learn to type in Faroese very fast.",
	"version": "0.5.5",
	"manifest_version": 2,
	"minimum_chrome_version": "30",
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"icons": { "16": "icon-16.png", "128": "icon-128.png" },
	"offline_enabled": true,
	"permissions": [
		"storage",
		"notifications",
		"unlimitedStorage",
		"<all_urls>",
		"http://lysingar.tokni.fo/*",
		"webview"
	]
}